<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
  <%- include('./partials/nav') %>
  <h1>Full Menu</h1>
  <ul>
    <% menu.forEach(function(item) { %>
      <li>
        <h2><%= item.name %> - $<%= item.price %></h2>
        <p>Rating: <%= item.rating %></p>
        <p><%= item.details %></p>
      </li>
    <% }) %>
  </ul>
</body>
</html>
